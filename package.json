{
  "type": "module",
  "name": "@hazae41/x509",
  "version": "1.1.1",
  "description": "Zero-copy X.509 certificates in pure modern TypeScript",
  "homepage": "https://github.com/hazae41/x509",
  "repository": "github:hazae41/x509",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/esm",
    "./dist/cjs",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "node ./dist/test/index.test.mjs",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@hazae41/asn1": "^1.2.15",
    "@hazae41/binary": "^1.2.29",
    "@hazae41/bytes": "^1.1.10",
    "@hazae41/cursor": "^1.1.14",
    "@hazae41/result": "^1.0.26"
  },
  "devDependencies": {
    "@hazae41/phobos": "^1.0.10",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.2.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-node-externals": "^6.0.1",
    "typescript": "^5.0.4"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "x509",
    "pem",
    "certificates",
    "encoding",
    "decoding",
    "buffer",
    "bytes",
    "packets",
    "stream",
    "encoder",
    "decoder",
    "parsing",
    "parser",
    "asn1",
    "der",
    "ber",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ]
}
